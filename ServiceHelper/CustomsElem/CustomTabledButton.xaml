<UserControl x:Class="ServiceHelper.CustomsElem.CustomTabledButton"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ServiceHelper.CustomsElem"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800" Width="160" Height="80">
    <Button x:Name="tableButton" Margin="0" FontSize="16" Foreground="#FFFAD29B" Background="#FFFAD29B">
        <Button.Effect>
            <DropShadowEffect BlurRadius="10" ShadowDepth="0"/>
        </Button.Effect>
        <TextBlock x:Name="tableText" FontSize="20" Text="Стол" FontWeight="SemiBold" Foreground="#FF8C6432"/>
    </Button>
</UserControl>
